{
  "name": "bp-request",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
